<%- include('../partials/header') %>

<h2>Flights</h2>
<table>
    <thead>
        <th>Airline</th>
        <th>Airport</th>
        <th>flight Number</th>
        <th>Departs</th>
    </thead>
    <tbody>
        <% flights.forEach(function(f) { %>
            <tr>
                <td><%= f.airline %></td>
                <td><%= f.airport %></td>
                <td><%= f.flightNo %></td>
                <td><%= f.departs %></td>
            </tr>
            <% }); %>
    </tbody>
</table>
<%- include('../partials/footer') %>